<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:background="@color/green"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/ic_launcher_foreground"
        android:label="Restaurante Sabor de Casa"
        android:roundIcon="@drawable/cuchilleria"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProyectoFinal"
        tools:targetApi="31">
        <activity
            android:name=".ClientesActivity"
            android:exported="false" />
        <activity
            android:name=".CompletarActivity"
            android:exported="false" />
        <activity
            android:name=".PlatillosActivity"
            android:exported="false" />
        <activity
            android:name=".PlanitillosActivity"
            android:exported="false"
            android:theme="@style/Theme.ProyectoFinal" />
        <activity
            android:name=".FormularioActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>